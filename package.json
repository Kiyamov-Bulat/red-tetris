{
  "name": "red_tetrisboilerplate",
  "version": "0.0.1",
  "author": "redpelicans",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint src",
    "dev": "npm run client-watch & npm run srv-dev",
    "dist": "npm run client-dist && npm run srv-dist",
    "client-watch": "webpack -w",
    "client-dev": "webpack serve --open",
    "client-dist": "cross-env NODE_ENV=production webpack --progress --colors --mode=production",
    "srv-dev": "DEBUG=tetris:* babel-watch -w src src/server/main.js",
    "srv-dist": "DEBUG=tetris:* babel src --out-dir dist",
    "jest": "jest --config jest.conf.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.16.7",
    "@babel/register": "^7.21.0",
    "@testing-library/react": "^14.0.0",
    "babel-jest": "^29.4.3",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-watch": "^7.7.2",
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^7.32.0",
    "eslint-loader": "^4.0.2",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "jsdom": "^21.1.0",
    "mini-css-extract-plugin": "^2.6.0",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.52.1",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "webpack": "5.72.1",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.8.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9.3",
    "classnames": "2.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5",
    "redux": "4.1.2",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "uuid": "8.3.2"
  }
}
